{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "autocomplete_get/songsFacet.json",
    "type": "object",
    "title": "Songs Facet Schema",
    "description": "Matches the autocomplete.get.songs ",        
    "required": [
        "songs"
    ],
    "additionalProperties": true,
    "properties": {
        "songs": {
            "$id": "#/properties/songs",
            "type": "object",
            "title": "The songs schema",
            "description": "An explanation about the purpose of this instance.",                        
            "required": [
                "data",
                "position"
            ],
            "additionalProperties": true,
            "properties": {
                "data": {
                    "$id": "#/properties/songs/properties/data",
                    "type": "array",                    
                    "default": [],                    
                    "additionalItems": true,
                    "items": {
                        "anyOf": [{"$ref": "songData.schema.json#/definitions/data"}],
                        "$id": "#/properties/songs/properties/data/items"
                    }
                },
                "position": {
                    "$id": "#/properties/songs/properties/position",
                    "type": "integer"
                }
            }
        }
    }
}