{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "autocomplete_get/albumsFacet.json",
    "type": "object",
    "title": "Albums Facet Schema",
    "description": "Matches the autocomplete.get.albums ",        
    "required": [
        "albums"
    ],
    "additionalProperties": true,
    "properties": {
        "albums": {
            "$id": "#/properties/albums",
            "type": "object",
            "title": "The albums schema",
            "description": "An explanation about the purpose of this instance.",                        
            "required": [
                "data",
                "position"
            ],
            "additionalProperties": true,
            "properties": {
                "data": {
                    "$id": "#/properties/albums/properties/data",
                    "type": "array",
                    "title": "The data schema",
                    "description": "An explanation about the purpose of this instance.",
                    "default": [],                    
                    "additionalItems": true,
                    "items": {
                        "anyOf": [{"$ref": "albumData.schema.json#/definitions/data"}],
                        "$id": "#/properties/albums/properties/data/items"
                    }
                },
                "position": {
                    "$id": "#/properties/albums/properties/position",
                    "type": "integer"
                }
            }
        }
    }
}