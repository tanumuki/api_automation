{
    "$schema": "http://json-schema.org/draft-07/schema",
    
    "type": "object",
    "title": "Albums MoreInfo Schema",
    "description": "This schema matches the autocomplete.get.albums.more_info",
    "required": [
        "music",
        "ctr",
        "year",
        "is_movie",
        "language",
        "song_pids"
    ],
    "additionalProperties": true,
    "definitions": {
    	"more_info": {
    		"type" : "object",
    		"properties": {
		        "music": {
		            "$id": "#/definitions/properties/music",
		            "type": "string",            
		            "pattern": ".*"
		        },
		        "ctr": {
		            "$id": "#/definitions//properties/ctr",
		            "type": "integer"
		        },
		        "year": {
		            "$id": "#/definitions/properties/year",
		            "type": "string",            
		            "pattern": "^$|^[0-9]\\d*(\\.\\d+)?$|\\d+"
		        },
		        "is_movie": {
		            "$id": "#/definitions/properties/is_movie",
		            "type": "string"
		        },
		        "language": {
		            "$id": "#/definitions/properties/language",
		            "type": "string",            
		            "pattern": ".+"
		        },
		        "song_pids": {
		            "$id": "#/definitions/properties/song_pids",
		            "type": "string",            
		            "pattern": ".*"
		        }
		    }
    	}
    
    }
    
    
    
}